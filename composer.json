{
  "name": "arquivei/laravel-health-checker",
  "description": "Laravel Health Checker",
  "license": "Apache-2.0",
  "keywords": [
    "health",
    "health-checker",
    "laravel"
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": ">5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.4",
    "laravel/laravel": "^5.3",
    "predis/predis": "^1.0",
    "league/flysystem-aws-s3-v3": "^1.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "~6.0",
    "predis/predis": "^1.0",
    "league/flysystem-aws-s3-v3": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Health\\": "src/",
      "Laravel\\Health\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Health\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "@composer install",
      "vendor/bin/phpunit"
    ]
  }
}
